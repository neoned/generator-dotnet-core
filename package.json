{
  "name": "@maordavidov/generator-dotnet",
  "version": "1.0.0",
  "description": ".NET Core project template generator",
  "author": {
    "name": "Maor Davidov",
    "email": "maordavidov@gmail.com",
    "url": "https://github.com/maordavidov/generator-dotnet-core"
  },
  "publishConfig":{
    "access": "public"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "dotnet-core",
    "dotnetcore",
    "aspnet",
    "aspnetcore",
    "aspnet-core",
    "yeoman-generator",
    "dotnet",
    "dotnet core"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^2.8.0",
    "eslint": "^4.19.1",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.20.1",
    "jest": "^22.0.6"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "uuid": "^3.2.1",
    "vs_projectname": "^1.0.2",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "maordavidov/generator-dotnet-core",
  "license": "MIT"
}
